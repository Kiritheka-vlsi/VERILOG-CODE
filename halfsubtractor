#! /usr/bin/vvp
:ivl_version "11.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x56b60e0060d0 .scope module, "halfsubtractor_tb" "halfsubtractor_tb" 2 1;
 .timescale 0 0;
v0x56b60e019290_0 .net "b", 0 0, L_0x56b60e019790;  1 drivers
v0x56b60e019350_0 .net "d", 0 0, L_0x56b60e0195c0;  1 drivers
v0x56b60e0193f0_0 .var "x", 0 0;
v0x56b60e0194f0_0 .var "y", 0 0;
S_0x56b60e006260 .scope module, "DUT" "halfsubtractor" 2 6, 3 1 0, S_0x56b60e0060d0;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "x";
    .port_info 1 /INPUT 1 "y";
    .port_info 2 /OUTPUT 1 "d";
    .port_info 3 /OUTPUT 1 "b";
L_0x56b60e0195c0 .functor XOR 1, v0x56b60e0193f0_0, v0x56b60e0194f0_0, C4<0>, C4<0>;
L_0x56b60e019720 .functor NOT 1, v0x56b60e0193f0_0, C4<0>, C4<0>, C4<0>;
L_0x56b60e019790 .functor AND 1, L_0x56b60e019720, v0x56b60e0194f0_0, C4<1>, C4<1>;
v0x56b60dff4fc0_0 .net *"_ivl_2", 0 0, L_0x56b60e019720;  1 drivers
v0x56b60e018ee0_0 .net "b", 0 0, L_0x56b60e019790;  alias, 1 drivers
v0x56b60e018fa0_0 .net "d", 0 0, L_0x56b60e0195c0;  alias, 1 drivers
v0x56b60e019040_0 .net "x", 0 0, v0x56b60e0193f0_0;  1 drivers
v0x56b60e019100_0 .net "y", 0 0, v0x56b60e0194f0_0;  1 drivers
    .scope S_0x56b60e0060d0;
T_0 ;
    %vpi_call 2 13 "$monitor", "$time=%0t x=%b y=%b | d=%b b=%b", $time, v0x56b60e0193f0_0, v0x56b60e0194f0_0, v0x56b60e019350_0, v0x56b60e019290_0 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x56b60e0193f0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x56b60e0194f0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x56b60e0193f0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x56b60e0194f0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x56b60e0193f0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x56b60e0194f0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x56b60e0193f0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x56b60e0194f0_0, 0, 1;
    %delay 10, 0;
    %vpi_call 2 20 "$finish" {0 0 0};
    %end;
    .thread T_0;
# The file index is used to find the file name in the following table.
:file_names 4;
